<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>Item</servlet-name>
    <servlet-class>com.cohort.action.ItemAction</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ItemCategory</servlet-name>
    <servlet-class>com.cohort.action.ItemCategoryAction</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Item</servlet-name>
    <url-pattern>/item</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ItemCategory</servlet-name>
    <url-pattern>/itemcategory</url-pattern>
  </servlet-mapping>

</web-app>
